        "tasks": [
            {
                "id": "t1-1",
                "title": "Akses CLI Router",
                "description": "Buka terminal untuk Router1 dan masuk ke mode privileged menggunakan perintah 'enable'",
                "points": 10,
                "order": 1,
                "validation": { "type": "command-executed", "device": "router1", "command": "enable" }
            },
            {
                "id": "t1-2",
                "title": "Lihat Status Interface",
                "description": "Gunakan perintah 'show ip interface brief' untuk melihat semua interface",
                "points": 15,
                "order": 2,
                "validation": { "type": "command-executed", "device": "router1", "command": "show ip interface brief" }
            },
            {
                "id": "t1-3",
                "title": "Konfigurasi Alamat IP PC1",
                "description": "Atur alamat IP PC1 ke 10.1.1.2 dengan mask 255.255.255.0 dan gateway 10.1.1.1",
                "points": 20,
                "order": 3,
                "validation": { "type": "config-check", "device": "pc1", "expected": { "ip": "10.1.1.2", "gateway": "10.1.1.1" } }
            },
            {
                "id": "t1-4",
                "title": "Uji Konektivitas Lokal",
                "description": "Ping dari PC1 ke interface GigabitEthernet0/0 router (10.1.1.1)",
                "points": 15,
                "order": 4,
                "validation": { "type": "connectivity-test", "from": "pc1", "to": "10.1.1.1" }
            },
            {
                "id": "t1-5",
                "title": "Lihat Tabel Routing",
                "description": "Tampilkan tabel routing pada Router1 menggunakan 'show ip route'",
                "points": 15,
                "order": 5,
                "validation": { "type": "command-executed", "device": "router1", "command": "show ip route" }
            },
            {
                "id": "t1-6",
                "title": "Uji Konektivitas End-to-End",
                "description": "Ping dari PC1 ke PC2 (10.2.1.2) melalui router",
                "points": 25,
                "order": 6,
                "validation": { "type": "connectivity-test", "from": "pc1", "to": "10.2.1.2" }
            }
        ],
        "hints": [
            { "id": "h1-1", "taskId": "t1-1", "content": "Double-click pada Router1 untuk membuka terminal, lalu ketik 'enable' dan tekan Enter", "pointCost": 2, "order": 1 },
            { "id": "h1-2", "taskId": "t1-3", "content": "Gunakan perintah: set ip 10.1.1.2 255.255.255.0 10.1.1.1", "pointCost": 3, "order": 1 },
            { "id": "h1-3", "taskId": "t1-4", "content": "Gunakan perintah: ping 10.1.1.1", "pointCost": 2, "order": 1 }
        ]
    },
    {
        "id": "lab-2",
        "number": 2,
        "title": "TCP/IP Protocol Analysis",
