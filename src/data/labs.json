        "objectives": [
            "Menangkap paket jaringan menggunakan tools bawaan",
            "Menganalisis TCP three-way handshake",
            "Memahami field header IP",
            "Mengidentifikasi protokol umum (HTTP, DNS, ICMP)"
        ],
        "difficulty": "BEGINNER",
        "durationMinutes": 45,
        "maxScore": 100,
        "isLocked": false,
        "prerequisite": "lab-1",
        "topology": {
            "devices": [
                { "id": "pc1", "type": "pc", "name": "Client", "hostname": "Client", "x": 100, "y": 200, "initialConfig": { "ip": "192.168.1.10", "mask": "255.255.255.0", "gateway": "192.168.1.1" } },
                { "id": "router1", "type": "router", "name": "Gateway", "hostname": "GW", "x": 400, "y": 200, "initialConfig": { "interfaces": [{ "name": "GigabitEthernet0/0", "ip": "192.168.1.1", "mask": "255.255.255.0" }, { "name": "GigabitEthernet0/1", "ip": "10.0.0.1", "mask": "255.255.255.0" }] } },
                { "id": "server", "type": "pc", "name": "Server", "hostname": "Server", "x": 700, "y": 200, "initialConfig": { "ip": "10.0.0.100", "mask": "255.255.255.0", "gateway": "10.0.0.1" } }
            ],
            "links": [
                { "source": { "device": "pc1", "interface": "Ethernet0" }, "destination": { "device": "router1", "interface": "GigabitEthernet0/0" } },
                { "source": { "device": "router1", "interface": "GigabitEthernet0/1" }, "destination": { "device": "server", "interface": "Ethernet0" } }
            ]
        },
        "tasks": [
            { "id": "t2-1", "title": "Mulai Penangkapan Paket", "description": "Aktifkan penangkapan paket pada PC Client", "points": 10, "order": 1, "validation": { "type": "command-executed", "device": "pc1", "command": "capture start" } },
            { "id": "t2-2", "title": "Generate Lalu Lintas ICMP", "description": "Ping server untuk menghasilkan paket ICMP", "points": 15, "order": 2, "validation": { "type": "connectivity-test", "from": "pc1", "to": "10.0.0.100" } },
            { "id": "t2-3", "title": "Analisis Paket ICMP", "description": "Lihat paket ICMP yang ditangkap dan identifikasi echo request/reply", "points": 20, "order": 3, "validation": { "type": "packet-capture", "protocol": "ICMP" } },
            { "id": "t2-4", "title": "Generate Lalu Lintas TCP", "description": "Mulai koneksi TCP ke server pada port 80", "points": 20, "order": 4, "validation": { "type": "command-executed", "device": "pc1", "command": "telnet 10.0.0.100 80" } },
            { "id": "t2-5", "title": "Identifikasi TCP Handshake", "description": "Temukan paket SYN, SYN-ACK, dan ACK dalam tangkapan", "points": 25, "order": 5, "validation": { "type": "packet-analysis", "expected": ["SYN", "SYN-ACK", "ACK"] } },
            { "id": "t2-6", "title": "Ekspor Laporan Analisis", "description": "Simpan laporan analisis penangkapan paket", "points": 10, "order": 6, "validation": { "type": "command-executed", "device": "pc1", "command": "capture export" } }
        ],
        "hints": [
            { "id": "h2-1", "taskId": "t2-1", "content": "Gunakan perintah 'capture start' untuk memulai penangkapan paket", "pointCost": 2, "order": 1 },
            { "id": "h2-2", "taskId": "t2-5", "content": "Cari flag TCP: SYN=0x02, SYN-ACK=0x12, ACK=0x10", "pointCost": 4, "order": 1 }
        ]
    },
    {
        "id": "lab-3",
